from discord.client import DiscordClient
from discord.utils import get

client = DiscordClient(command_prefix='')

@client.event
async def on_ready():
    print('Logged in as')
    print(client.user.name)
    print(client.user.id)

    guild = get(client.guilds, name='my-guild')
    guild.icon = 'url_of_devil_with_computer_image'

    print('Logo set to')
    print(guild.icon)

client.run('token')
